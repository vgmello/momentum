{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "AppDomain/bin/**/**/AppDomain.dll",
            "AppDomain.Api/bin/**/**/AppDomain.Api.dll",
            "AppDomain.BackOffice/bin/**/**/AppDomain.BackOffice.dll",
            "AppDomain.BackOffice.Orleans/bin/**/**/AppDomain.BackOffice.Orleans.dll"
          ]
        }
      ],
      "output": "reference/",
      "outputFormat": "markdown",
      "namespaceLayout": "nested",
      "categoryLayout": "nested"
    }
  ]
}
