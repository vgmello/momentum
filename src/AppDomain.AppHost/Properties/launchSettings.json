{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "https://localhost:$AspireHttpsPort;http://localhost:aspireHttpPort",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "ASPIRE_DASHBOARD_CORS_ALLOWED_ORIGINS": "*",
        "ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "http://localhost:4317",
        "ASPIRE_DASHBOARD_OTLP_HTTP_ENDPOINT_URL": "http://localhost:4318",
        "DOTNET_RESOURCE_SERVICE_ENDPOINT_URL": "https://localhost:SERVICE_BASE_PORT"
      }
    },
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:aspireHttpPort",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "ASPIRE_DASHBOARD_CORS_ALLOWED_ORIGINS": "*",
        "ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "http://localhost:4317",
        "ASPIRE_DASHBOARD_OTLP_HTTP_ENDPOINT_URL": "http://localhost:4318",
        "DOTNET_RESOURCE_SERVICE_ENDPOINT_URL": "http://localhost:SERVICE_BASE_PORT",
        "ASPIRE_ALLOW_UNSECURED_TRANSPORT": "true"
      }
    }
  }
}
