{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Error",
      "Wolverine": "Error",
      "LinqToDB": "Error",
      "AppDomain": "Error"
    }
  },
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 25000,
      "MaxConcurrentUpgradedConnections": 25000,
      "MaxRequestBodySize": 10485760,
      "MinRequestBodyDataRate": {
        "BytesPerSecond": 100,
        "GracePeriod": "00:00:10"
      },
      "MinResponseDataRate": {
        "BytesPerSecond": 100,
        "GracePeriod": "00:00:10"
      },
      "KeepAliveTimeout": "00:02:00",
      "RequestHeadersTimeout": "00:00:20"
    },
    "RequestTimeout": "00:00:15",
    "AddServerHeader": false
  },
  "ConnectionStrings": {
    "AppDomainDb": "Host=localhost;Port=54320;Database=app_domain;Username=postgres;Password=password@;Pooling=true;MinPoolSize=50;MaxPoolSize=150;CommandTimeout=30;ConnectionIdleLifetime=300;ConnectionPruningInterval=10;NoResetOnClose=true;Multiplexing=true;Application Name=AppDomain-Performance;Enlist=false",
    "ServiceBus": "Host=localhost;Port=54320;Database=service_bus;Username=postgres;Password=password@;Pooling=true;MinPoolSize=10;MaxPoolSize=50;CommandTimeout=30;ConnectionIdleLifetime=300;ConnectionPruningInterval=10;NoResetOnClose=true;Multiplexing=true;Application Name=ServiceBus-Performance;Enlist=false",
    "Messaging": "localhost:29092"
  },
  "Wolverine": {
    "OptimizeArtifactWorkflow": "true",
    "CodeGeneration": "Static",
    "MaximumEnvelopeRetryStorage": 100000,
    "LocalQueue": {
      "MaximumParallelization": 500,
      "ExecutionTimeout": "00:00:10"
    },
    "Transports": {
      "Durability": {
        "Mode": "Solo",
        "CheckpointFrequency": 10000,
        "NodeReassignmentPolling": "00:10:00"
      },
      "Local": {
        "MaximumParallelization": 500,
        "MaximumAttempts": 1
      }
    },
    "Discovery": {
      "DisableConventionalDiscovery": true
    }
  },
  "AllowedHosts": "*"
}
