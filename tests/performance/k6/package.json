{
  "name": "momentum-k6-performance-tests",
  "version": "1.0.0",
  "description": "k6 performance tests for .NET microservices",
  "private": true,
  "scripts": {
    "test:local": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-local-realistic-workflow-report.html k6 run scenarios/mixed/realistic-workflow.js",
    "test:staging": "ENVIRONMENT=staging K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-staging-realistic-workflow-report.html k6 run scenarios/mixed/realistic-workflow.js",
    "test:cashiers": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-cashiers-baseline-report.html k6 run scenarios/cashiers/baseline.js",
    "test:cashiers:simple": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-cashiers-simple-create-report.html k6 run scenarios/cashiers/simple-create.js",
    "test:cashiers:stress": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-cashiers-stress-report.html k6 run scenarios/cashiers/stress.js",
    "test:cashiers:spike": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-cashiers-spike-report.html k6 run scenarios/cashiers/spike.js",
    "test:invoices": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-invoices-baseline-report.html k6 run scenarios/invoices/baseline.js",
    "test:mixed": "K6_WEB_DASHBOARD=true K6_WEB_DASHBOARD_EXPORT=../results/k6-mixed-realistic-workflow-report.html k6 run scenarios/mixed/realistic-workflow.js"
  },
  "dependencies": {
    "k6": "^0.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
